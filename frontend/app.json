{
  "expo": {
    "name": "riptfitness",
    "slug": "Ript",
    "owner": "sjiang1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./final-2nd-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./ript_workouts.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.RiptFitness.Ript"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./final-2nd-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.RiptFitness.Ript"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 34,
            "buildToolsVersion": "35.0.0",
            "kotlinVersion": "1.9.25"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "37565054-23c6-429e-9b9c-8999008e91ba"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/37565054-23c6-429e-9b9c-8999008e91ba"
    }
  }
}
